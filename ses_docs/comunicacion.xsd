<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://www.soap.servicios.hospedajes.mir.es/comunicacion" xmlns="http://www.soap.servicios.hospedajes.mir.es/comunicacion" xmlns:xsd="http://www.w3.org/2001/XMLSchema"			xmlns:com="http://www.soap.servicios.hospedajes.mir.es/tipoComunicacion" version="3.1.1">	<xsd:import namespace="http://www.soap.servicios.hospedajes.mir.es/tipoComunicacion" schemaLocation="tipoComunicacion.xsd"/>	<xsd:element name="consultaComunicacionRequest">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="codigos" type="com:CodigosComunicacionType"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="consultaComunicacionResponse">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="resultado" type="com:ResultadoRespuestaType"/>				<xsd:element name="comunicacion" type="com:comunicacionType" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="consultaLoteRequest">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="codigosLote" type="com:codigosLoteType"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="consultaLoteResponse">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="respuesta" type="com:ResultadoRespuestaType"/>				<xsd:element name="resultado" type="com:wscomunicacionType" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="anulacionLoteRequest">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="lote" type="com:CodigoLoteType"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="anulacionLoteResponse">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="codigo" type="xsd:int"/>				<xsd:element name="descripcion" type="xsd:string"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="comunicacionRequest">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="peticion" type="com:peticionType"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="comunicacionResponse">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="respuesta" type="com:estadoRespuestaType"/>				<xsd:element name="resultado" type="com:wscomunicacionType" minOccurs="0" maxOccurs="unbounded"/>			</xsd:sequence>		</xsd:complexType>	</xsd:element>	<xsd:element name="catalogoRequest">        <xsd:complexType>            <xsd:sequence>                <xsd:element name="peticion" type="catalogoRequestType"/>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:element name="catalogoResponse">        <xsd:complexType>            <xsd:sequence>                <xsd:element name="resultado" type="com:ResultadoRespuestaType"/>                <xsd:element name="respuesta" type="catalogoResponseType"/>            </xsd:sequence>        </xsd:complexType>    </xsd:element>    <xsd:complexType name="catalogoResponseType">		<xsd:sequence>			<xsd:element name="resultado" type="resultadoType"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="resultadoType">		<xsd:sequence>			<xsd:element name="tupla" type="catalogoType" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="catalogoType">		<xsd:sequence>			<xsd:element name="codigo" type="xsd:string"/>			<xsd:element name="descripcion" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType>		<xsd:complexType name="catalogoRequestType">		<xsd:sequence>			<xsd:element name="catalogo" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType></xsd:schema>